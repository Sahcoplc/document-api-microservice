{
  "name": "document-api-microservice",
  "version": "1.0.1",
  "description": "SAHCO Document Microservice API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --testTimeout=10000 --watch",
    "start": "nodemon src/index.js",
    "serve": "nodemon src/index.js",
    "semantic-release": "semantic-release",
    "prettier-format": "prettier --config .prettierrc \"**/**/*.js\" --write",
    "lint": "eslint . --ext .js",
    "lint-and-fix": "eslint . --ext .js --fix",
    "prettier-watch": "onchange '**/**/*.js' -- prettier --write {{changed}}",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sahcoplc/document-api-microservice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sahcoplc/document-api-microservice/issues"
  },
  "homepage": "https://github.com/Sahcoplc/document-api-microservice#readme",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "aws-sdk": "^2.1433.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.32.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "express-rate-limit": "^6.8.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.4.1",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.7.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.8.0",
    "nodemailer-sendinblue-transport": "^2.0.0",
    "nodemon": "^2.0.20",
    "papaparse": "^5.4.1",
    "rate-limit-mongo": "^2.3.2",
    "rate-limit-redis": "^3.0.2",
    "read-excel-file": "^5.6.1",
    "redis": "^4.6.7",
    "request": "^2.88.2",
    "rollbar": "^2.26.2",
    "scramjet": "^4.36.9",
    "socket.io": "^4.7.2",
    "toobusy-js": "^0.5.1",
    "util": "^0.12.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.2.2",
    "mocha": "^10.1.0",
    "semantic-release": "^19.0.5",
    "sinon": "^14.0.1"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist"
        }
      ],
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  }
}
